@page "/"

<ItemFilterView Model=@Filter OnChange=@OnChangeFilter />

<PaginationView Count=@Page.Count Cursor=@Cursor OnChange=@OnChangeCursor />

<Row Gutter=@((8, 8))>
    @foreach (var item in Page.Items) 
    {
        <AntDesign.Col Span="4">
            <ItemCard Item=@item/>
        </AntDesign.Col>
    }
</Row>